<UserControl
    x:Class="TaoBD10.Views.ChinhView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TaoBD10.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{StaticResource ChinhVM}"
    mc:Ignorable="d">
    <StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="Lấy Dữ Liệu"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
            <Button
                Width="115"
                Margin="5"
                Content="593230"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding KTHNCommand, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="Kiện"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding KienCommand, Mode=OneWay}" />
            <Button
                Width="115"
                Margin="5"
                Content="EMS"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding EMSCommand, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="Tam Quan"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
            <Button
                Width="115"
                Margin="5"
                Content="Qui Nhơn 1"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding QuiNhon1Command, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="Qui Nhơn 2"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding QuyNhon2Command, Mode=OneWay}" />
            <Button
                Width="115"
                Margin="5"
                Content="Phù Mỹ"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding PhuMyCommand, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Command="{Binding PhuCatCommand}"
                Content="Phù Cát"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
            <Button
                Width="115"
                Margin="5"
                Content="An Nhơn"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding AnNhonCommand, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="593280"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding BCPHNCommand, Mode=OneWay}" />
            <Button
                Width="115"
                Margin="5"
                Content="Tổng Hợp"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" Command="{Binding TongHopCommand, Mode=OneWay}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" />
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="An Hòa"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
            <Button
                Width="115"
                Margin="5"
                Content="Ân Mỹ"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button
                Width="115"
                Margin="5"
                Content="An Lão"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
            <Button
                Width="115"
                Margin="5"
                Content="An Hòa"
                IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                Style="{StaticResource MaterialDesignPaperButton}" />
        </StackPanel>
    </StackPanel>
</UserControl>
