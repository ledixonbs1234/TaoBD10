<UserControl
    x:Class="TaoBD10.Views.LayBDHA"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TaoBD10.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
   
    DataContext="{StaticResource LayBDHAVM}"
    mc:Ignorable="d">
    <StackPanel>
        <Button Margin="5,20,5,5" Content="Hoài Ân" Command="{Binding HoaiAnCommand}"/>
        <Button Margin="5" Content="Ân Mỹ" Command="{Binding AnMyCommand}"/>
        <Button Margin="5" Content="An Lão" Command="{Binding AnLaoCommand}"/>
        <Button Margin="5" Content="An Hòa" Command="{Binding AnHoaCommand}"/>
    </StackPanel>
</UserControl>
